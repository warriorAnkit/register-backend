{
  "name": "server-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "db:migrate": "./node_modules/.bin/sequelize --options-path ./src/schema/main-server/.sequelizerc db:migrate",
    "dev": "nodemon src/",
    "dev-docker:up": "docker-compose -f docker-compose.dev.yml --env-file ./docker/.env.docker up -d --build",
    "dev-docker:down": "docker-compose -f docker-compose.dev.yml --env-file ./docker/.env.docker down",
    "dev-docker:logs": "docker logs -f server-boilerplate-dev",
    "dev-docker:install": "docker exec -it server-boilerplate-dev npm i",
    "lint": "esw --ext .json,.js,.gql,.graphql src --color",
    "lint:watch": "npm run lint -- --watch",
    "lint:fix": "eslint src --ext .json,.js,.gql,.graphql --fix",
    "pre-commit": "npm run lint",
    "pre-push": "lint-staged && npm run lint",
    "predev-docker:up": "sh ./docker/startup.sh",
    "prestart": "npm run db:migrate",
    "start": "node src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "postinstall": "npm install pg",
    "build": "node-gyp rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:logicwind/general/backend/server-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "4.10.2",
    "@aws-sdk/client-cloudfront": "3.554.0",
    "@aws-sdk/client-s3": "3.554.0",
    "@aws-sdk/cloudfront-signer": "3.541.0",
    "@aws-sdk/s3-request-presigner": "3.554.0",
    "@graphql-tools/load-files": "7.0.0",
    "@graphql-tools/merge": "9.0.3",
    "@graphql-tools/schema": "10.0.3",
    "@graphql-tools/utils": "10.1.2",
    "@mux/mux-node": "7.3.3",
    "@sentry/node": "7.86.0",
    "apollo-server": "^3.13.0",
    "axios": "1.6.8",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "crypto": "1.0.1",
    "dataloader": "2.2.2",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "express-rate-limit": "7.2.0",
    "express-useragent": "1.0.15",
    "graphql": "16.8.1",
    "graphql-query-complexity": "0.12.0",
    "graphql-rate-limit": "3.3.0",
    "graphql-redis-subscriptions": "2.6.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-ws": "5.16.0",
    "http-status": "1.7.4",
    "i18n": "0.15.1",
    "ioredis": "5.3.2",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "mime": "3.0.0",
    "moment": "2.30.1",
    "multer": "1.4.5-lts.1",
    "nanoid": "5.0.7",
    "pg": "^8.13.1",
    "pg-hstore": "2.3.4",
    "pusher": "5.2.0",
    "qs": "6.12.0",
    "randomstring": "1.3.0",
    "rate-limit-redis": "4.2.0",
    "sequelize": "6.37.2",
    "sequelize-cli": "6.6.2",
    "uuid": "9.0.1",
    "winston": "3.13.0",
    "ws": "8.16.0"
  },
  "devDependencies": {
    "@graphql-eslint/eslint-plugin": "3.20.1",
    "eslint": "8.55.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-eslint-plugin": "5.2.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-security": "1.7.1",
    "eslint-watch": "8.0.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.0",
    "nodemon": "3.1.0"
  },
  "engines": {
    "node": "20.18.1",
    "npm": "10.8.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  }
}
